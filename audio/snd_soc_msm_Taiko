	Name "FM A2DP REC"

	EnableSequence
		'MultiMedia2 Mixer INTERNAL_FM_TX':1:1
	EndSequence

	DisableSequence
		'MultiMedia2 Mixer INTERNAL_FM_TX':1:0
	EndSequence
	CapturePCM  1
	PlaybackPCM  1
EndSection
}
