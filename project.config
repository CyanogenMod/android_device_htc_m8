[access]
	inheritFrom = All-Projects
[submit]
	action = rebase if necessary
[access "refs/heads/*"]
	create = group m8 Maintainers
	push = group m8 Maintainers
	label-Code-Review = -2..+2 group m8 Maintainers
	label-Verified = -1..+1 group m8 Maintainers
	rebase = group m8 Maintainers
	submit = group m8 Maintainers
